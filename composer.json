{
    "name": "br24vietnam/downloaduploadtool",
    "type": "project",
    "description": "BR24 Download Upload Tool.",
    "keywords": [
        "downloaduploadtool",
        "laravel",
        "sigmoswitch",
        "br24vietnam"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
